#version 300 es
#line 1
/* 
 *
 * Fragment shader for glTF asset
 * @author Richard Sahlin
 */
precision highp float;

void main() {
    outputPixel(texture(uTexture0, vTexCoord0) * 
        vec4(calculateFresnelDiffuse(
            vec3(texture(uTextureNormal, vTexNormal) * 2.0 - 1.0) * mTangentLight), 1.0));
}
