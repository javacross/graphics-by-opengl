#version 300 es
/* 
 * Vertex shader for untextured glTF asset
 */
 precision highp float;

//Put array declaration after name for GLSL compatibility
uniform mat4 uModelMatrix[3];
uniform vec3 _LIGHT_0[1];

in vec3 POSITION;
in vec3 NORMAL;

out vec4 vDiffuseColor;
out vec3 vNormal;
out vec3 vEye;
out vec3 vLight;
out vec3 vLightOut;
out vec3 vU;

void positionLight();

void main() {
    positionLight();
}
