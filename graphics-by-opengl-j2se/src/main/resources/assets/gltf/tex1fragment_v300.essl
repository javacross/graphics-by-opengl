precision highp float;

void main() {
    vec3[2] diffuseSpecular = calculateFresnelDiffuse(vNormal);
    outputPixel(vec4(diffuseSpecular[0] * texture(uTexture0, vTexCoord0).rgb + diffuseSpecular[1], 1.0));
}
